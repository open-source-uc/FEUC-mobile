  "description": "FEUC mobile app",
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "ios": "node node_modules/react-native/local-cli/cli.js run-ios --simulator=\"iPhone 5\"",
    "android": "node node_modules/react-native/local-cli/cli.js run-android",
    "icon": "yarn run icon:android && yarn run icon:ios",
    "icon:android": "yo rn-toolbox:assets --android --icon resources/icons/android.png",
    "icon:ios": "yo rn-toolbox:assets --ios --icon resources/icons/ios.png",
    "link-fonts": "rnpm link assets",
    "upgrade": "yarn upgrade-interactive",
    "test": "jest",
    "lint": "eslint src",
    "clean": "watchman watch-del-all && rm -fr $TMPDIR/react-* && rm -rf node_modules && yarn install",
    "postversion": "react-native-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mrpatiwi/FEUC-api.git"
  },
